package com.df.Startup;

import com.df.FirstRun.FirstTimeRun;
import com.df.Global.Global_Vars;

public class Handler 
{
	public Handler()
	{
		gv = new Global_Vars();
		
		
		if (gv.usersDir.exists() && gv.usersDir.listFiles().length != 0)
			Login.main(null);
		else
			
	}
	
	public static void main(String[] args) 
	{
		new Handler();
	}

	Global_Vars gv = null;
}